func fizzbuzz(n int) void {
    let i = 1;
    while (i <= n) {
        if (i % 15 == 0) {
            println "FizzBuzz";
        } else {
            if (i % 3 == 0) {
                println "Fizz";
            } else {
                if (i % 5 == 0) {
                    println "Buzz";
                } else {
                    println i;
                }
            }
        }
        i = i + 1;
    }
}

func main() void {
    fizzbuzz(100);
}