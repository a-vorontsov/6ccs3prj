func main() void {
    let start int;
    start = 1000;

    let x = start;
    let y = start;
    let z = start;

    while (x > 0) {
        while (y > 0) {
            while (z > 0) {
                z = z - 1;
            }

            z = start;
            y = y - 1;
        }

        y = start;
        x = x - 1;
    }
}
