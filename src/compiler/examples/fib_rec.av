func fib(n : int) : int {
    if (n == 0) {
        return 0;
    } else {
        if (n == 1) {
            return 1;
        } else {
            return fib(n - 2) + fib(n - 1);
        }
    }
}

func main() : void {
    let i : int = 0;
    while (i < 31) {
        print fib(i);
        i = i + 1;
    }
}
