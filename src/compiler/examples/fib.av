func fib(n : int) : int {
    let i : int = 0;
    let nextterm : int = 1;
    let present : int = 1;
    let previous : int = 0;

    while (i < n) {
        nextterm = present + previous;
        present = previous;
        previous = nextterm;
        i = i + 1;
    }

    return nextterm;
}

func main() : void {
    let i : int = 0;
    while (i < 31) {
        print fib(i);
        i = i + 1;
    }
}
