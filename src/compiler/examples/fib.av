func fib(n) {
    i = 0;
    nextterm = 1;
    present = 1;
    previous = 0;

    while (i < n) {
        nextterm = present + previous;
        present = previous;
        previous = nextterm;
        i = i + 1;
    }

    return nextterm;
}

func main() {
    i = 0;
    while (i < 31) {
        print fib(i);
        i = i + 1;
    }
}
